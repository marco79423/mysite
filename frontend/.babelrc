{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"],
        "node": "current",
        "uglify": true
      },
      "modules": false,
      "debug": true
    }],
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "babel-plugin-styled-components"
  ],
  "env": {
    "test": {
      "presets": [
        "react",
        "env",
        "stage-0"
      ],
      "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "babel-plugin-styled-components"
      ]
    }
  }
}
